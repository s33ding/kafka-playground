{
  "name": "iceberg-bronze-s3-sink",
  "config": {
    "connector.class": "io.confluent.connect.s3.S3SinkConnector",
    "tasks.max": "3",
    "topics": "postgres-server.kafka.mcdonalds_sales,postgres-server.kafka.mcdonalds_inventory,postgres-server.kafka.mcdonalds_employees",
    "s3.region": "us-east-1",
    "s3.bucket.name": "s33ding-kafka-output",
    "flush.size": "10",
    "rotate.interval.ms": "60000",
    "storage.class": "io.confluent.connect.s3.storage.S3Storage",
    "format.class": "io.confluent.connect.s3.format.json.JsonFormat",
    "partitioner.class": "io.confluent.connect.storage.partitioner.DefaultPartitioner",
    "topics.dir": "db_bronze",
    "key.converter": "org.apache.kafka.connect.storage.StringConverter",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "value.converter.schemas.enable": "false"
  }
}
